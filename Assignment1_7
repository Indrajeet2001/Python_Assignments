def Chk(No):
    if (No % 5 == 0):
        return True
    else:
        return False

def main():

    print("Enter the number :")
    Num = int(input())

    output = Chk(Num)

    if(output == True):
        print("True")
    else:
        print("False")

if __name__ == "__main__":
    main()