def ChKNum(No):
    if (No % 2 == 0):
        return True
    else:
        return False

def main():

    print("Enter the number :")
    Num = int(input())

    result = ChKNum(Num)

    if(result == True):
        print("Even Number")
    else:
        print("Odd Number")

if __name__ == "__main__":
    main()